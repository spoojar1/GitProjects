<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="SuperKartWebServices.SuperKartWebServices.ReviewsBean" table="Reviews">  
    <id name="reviewId">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="reviewText"></property> 
    <property name="userName"></property> 
    <property name="productId"></property> 
    <property name="rating"></property> 
	<property name="reviewDate"></property> 
            
  </class>  
            
 </hibernate-mapping>  
