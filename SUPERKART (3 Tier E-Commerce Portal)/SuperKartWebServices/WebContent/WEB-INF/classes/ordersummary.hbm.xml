<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="SuperKartWebServices.SuperKartWebServices.OrderSummaryBean" table="OrderSummary">  
    <id name="orderId">  
		<generator class="assigned"></generator>  
    </id>  
             
    <property name="userName"></property> 
    <property name="totalBill"></property> 
    <property name="shipAddress"></property> 
	<property name="paymentMethod"></property> 
    <property name="orderDate"></property> 
    <property name="shippedDate"></property> 
	<property name="orderStatus"></property>
            
  </class>  
            
 </hibernate-mapping>  
 